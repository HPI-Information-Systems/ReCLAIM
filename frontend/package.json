  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:client": "npx --yes openapi-typescript-codegen --input http://localhost:8000/openapi.json --output ./src/lib/client --client axios --useOptions --useUnionTypes --name KunstgraphClient",
